<div class="row">
	<div class="col-md-3">
		<%= render 'skill_menu' %>
	</div>
	<div class="col-md-9">
	<div class="panel panel-default">
		<div class="panel-heading">
			Photos
		</div>

		<div class="panel-body">
			<div class="container">
				<div class="row">
					<div class="col-md-offset-3 col-md-6">
						<!-- PHOTOS UPLOAD GOES HERE -->
						<%= form_for @skill, url: skill_photos_path(@skill), method: 'post', html: {multipart: true} do |f| %>
						<div class="row">
							<div class="form-group">
								<span class="btn btn-default btn-file text-babu">
								<i class="fa fa-picture-o" aria-hidden="true"></i> Select Photos
									<%= file_field_tag "images[]", type: :file, multiple: true %>
								</span>
							</div>
						</div>

						<div class="text-center">
							 <%= f.button'<i class="fa fa-cloud-upload" aria-hidden="true"></i>  UPLOAD'.html_safe, class: "btn btn-form", id: "register-button" %>


						</div>
						<% end %>
					</div>
				</div>
				<div id="photos">
					<%= render 'photos/photos_list' %>
				</div>
			</div>
		</div>

	</div>
	</div>
</div>
























